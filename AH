						game:GetService("TweenService"):Create(Button5Local.Button.Text1,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextTransparency = 0}):Play()
						game:GetService("TweenService"):Create(Button5Local.Button.Text1,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextColor3=LeftBarButtonsTextsValue}):Play()
						game:GetService("TweenService"):Create(Button5Local.Button.Text2,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextTransparency = 0}):Play()
						game:GetService("TweenService"):Create(Button5Local.Button.Icon,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{ImageColor3=LeftBarButtonsShowValue}):Play()
						game:GetService("TweenService"):Create(Button5Local.Button.Text2,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextColor3=LeftBarButtonsTextsValue}):Play()
					else
						--s
					end
				end
			end
		end
		--Button2Local hide
	end
end)
