	ColorPicker.Frame:TweenPosition(UDim2.new(-0.021, 0,-0.129, 0))
	ColorPicker.Frame:TweenSize(UDim2.new(0, 230,0, 28))
	wait(1)
	ColorPicker.Frame.ImageButton.Visible=true
	ColorPicker.Frame.ImageButton2.Visible=true
end)
