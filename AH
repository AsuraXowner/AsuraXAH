	game:GetService("TweenService"):Create(PanelGui,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{ScrollBarThickness = 5}):Play()
	wait(4)
	game:GetService("TweenService"):Create(PanelGui,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{ScrollBarThickness = 4}):Play()
end)
