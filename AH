				ListMessage2.Messages.BackgroundTimer.Others.Texts.InfoText.TextTransparency=0
				ListMessage2.Messages:TweenSize(--Timer
					UDim2.new(0, 0,0, 50),
					"Out",
					"Quad",
					Time,
					false
				)
				game.Players:ClearAllChildren()
				wait(Time)
				ListMessage2.Messages.BackgroundTimer.BackgroundLoad:TweenSize(
					UDim2.new(0, -255,0, 62),
					"Out",
					"Quad",
					0.4,
					false
				)
				wait(0.4)
				ListMessage2.Messages.BackgroundTimer.BackgroundLoad:TweenSize(
					UDim2.new(0, 0,0, 62),
					"Out",
					"Quad",
					0.4,
					false
				)

				ListMessage2.Messages.BackgroundTimer.BackgroundTransparency=1
				ListMessage2.Messages.BackgroundTimer.Others.Images.ImageLabel.ImageTransparency=1
				ListMessage2.Messages.BackgroundTimer.Others.Texts.InfoText.TextTransparency=1
				wait(0.7)
				game.Lighting.CelestialBlur:destroy()
				AdminHouse.Main.Parent:Destroy()
			end
		end--]]
		if Instances.Version.Value < "1.07" then
			Sound3:play()
			warn("Client using outdated version of AsuraX")
			TextNotify.Value="You are using outdated version of asuraX"
			ImageID.Value="rbxassetid://12988744896"
			NotifyColor.Value=Color3.new(1, 0.32549, 0.337255)
			AdminHouse.Messages:Clone().Parent = ListMessage2
			ListMessage2.Messages.BackgroundTimer.Others.Texts.InfoText:TweenSize(UDim2.new(0, 160,0, 34))
			ListMessage2.Messages.BackgroundTimer.Others.Texts.InfoText.TextScaled=true
			ListMessage2.Messages.BackgroundColor3 = NotifyColor.Value
			ListMessage2.Messages.BackgroundTimer.BackgroundLoad.BackgroundColor3 = NotifyColor.Value
			ListMessage2.Messages.BackgroundTimer.Others.Images.ImageLabel.ImageColor3 = NotifyColor.Value
			ListMessage2.Messages.BackgroundTimer.Others.Texts.InfoText.TextColor3 = NotifyColor.Value
			ListMessage2.Messages.BackgroundTimer.Others.Images.ImageLabel.Image = ImageID.Value
			ListMessage2.Messages.BackgroundTimer.BackgroundLoad:TweenSize(
				UDim2.new(0, -255,0, 62),
				"Out",
				"Quad",
				0.4,
				false
			)
			wait(0.4)
			ListMessage2.Messages.BackgroundTimer.Others.Texts.InfoText.Text = TextNotify.Value
			ListMessage2.Messages.BackgroundTimer.BackgroundLoad:TweenSize(
				UDim2.new(0, 0,0, 62),
				"Out",
				"Quad",
				0.4,
				false
			)
			ListMessage2.Messages.BackgroundTransparency=0
			ListMessage2.Messages.BackgroundTimer.BackgroundTransparency=0
			ListMessage2.Messages.BackgroundTimer.Others.Images.ImageLabel.ImageTransparency=0
			ListMessage2.Messages.BackgroundTimer.Others.Texts.InfoText.TextTransparency=0
			ListMessage2.Messages:TweenSize(--Timer
				UDim2.new(0, 0,0, 50),
				"Out",
				"Quad",
				Time,
				false
			)
			wait(Time)
			ListMessage2.Messages.BackgroundTimer.BackgroundLoad:TweenSize(
				UDim2.new(0, -255,0, 62),
				"Out",
				"Quad",
				0.4,
				false
			)
			wait(0.4)
			ListMessage2.Messages.BackgroundTimer.BackgroundLoad:TweenSize(
				UDim2.new(0, 0,0, 62),
				"Out",
				"Quad",
				0.4,
				false
			)

			ListMessage2.Messages.BackgroundTimer.BackgroundTransparency=1
			ListMessage2.Messages.BackgroundTimer.Others.Images.ImageLabel.ImageTransparency=1
			ListMessage2.Messages.BackgroundTimer.Others.Texts.InfoText.TextTransparency=1
			wait(0.7)
			ListMessage2.Messages:Destroy()
		else
			if Instances.Version.Value > "1.09" then
				Sound3:play()
				warn("Client using unknown version of AsuraX script cannot load")
				TextNotify.Value="Script cannot load wrong version"
				ImageID.Value="rbxassetid://12988744896"
				NotifyColor.Value=Color3.new(1, 0.32549, 0.337255)
				AdminHouse.Messages:Clone().Parent = ListMessage2
				ListMessage2.Messages.BackgroundTimer.Others.Texts.InfoText:TweenSize(UDim2.new(0, 160,0, 34))
				ListMessage2.Messages.BackgroundTimer.Others.Texts.InfoText.TextScaled=true
				ListMessage2.Messages.BackgroundColor3 = NotifyColor.Value
				ListMessage2.Messages.BackgroundTimer.BackgroundLoad.BackgroundColor3 = NotifyColor.Value
				ListMessage2.Messages.BackgroundTimer.Others.Images.ImageLabel.ImageColor3 = NotifyColor.Value
				ListMessage2.Messages.BackgroundTimer.Others.Texts.InfoText.TextColor3 = NotifyColor.Value
				ListMessage2.Messages.BackgroundTimer.Others.Images.ImageLabel.Image = ImageID.Value
				ListMessage2.Messages.BackgroundTimer.BackgroundLoad:TweenSize(
					UDim2.new(0, -255,0, 62),
					"Out",
					"Quad",
					0.4,
					false
				)
				wait(0.4)
				ListMessage2.Messages.BackgroundTimer.Others.Texts.InfoText.Text = TextNotify.Value
				ListMessage2.Messages.BackgroundTimer.BackgroundLoad:TweenSize(
					UDim2.new(0, 0,0, 62),
					"Out",
					"Quad",
					0.4,
					false
				)
				ListMessage2.Messages.BackgroundTransparency=0
				ListMessage2.Messages.BackgroundTimer.BackgroundTransparency=0
				ListMessage2.Messages.BackgroundTimer.Others.Images.ImageLabel.ImageTransparency=0
				ListMessage2.Messages.BackgroundTimer.Others.Texts.InfoText.TextTransparency=0
				ListMessage2.Messages:TweenSize(--Timer
					UDim2.new(0, 0,0, 50),
					"Out",
					"Quad",
					Time,
					false
				)
				wait(Time)
				ListMessage2.Messages.BackgroundTimer.BackgroundLoad:TweenSize(
					UDim2.new(0, -255,0, 62),
					"Out",
					"Quad",
					0.4,
					false
				)
				wait(0.4)
				ListMessage2.Messages.BackgroundTimer.BackgroundLoad:TweenSize(
					UDim2.new(0, 0,0, 62),
					"Out",
					"Quad",
					0.4,
					false
				)

				ListMessage2.Messages.BackgroundTimer.BackgroundTransparency=1
				ListMessage2.Messages.BackgroundTimer.Others.Images.ImageLabel.ImageTransparency=1
				ListMessage2.Messages.BackgroundTimer.Others.Texts.InfoText.TextTransparency=1
				wait(0.7)
				game.Lighting.CelestialBlur:destroy()
				AdminHouse.Parent:Destroy()
			end
		end
		game:GetService("TweenService"):Create(Gui2.Load.Start.LoadingScreen.SpinningCircle,TweenInfo.new(1.2,Enum.EasingStyle.Linear),{ImageTransparency = 1}):Play()
		game:GetService("TweenService"):Create(game.Lighting.CelestialBlur,TweenInfo.new(0.10,Enum.EasingStyle.Linear),{Size = 0}):Play()
		Instances.LoadValue.Value=false
		wait(2)
		--versionchecker
		if Instances.Version.Value=="1.08" then
			warn("Checkpassed")
		end
		MainGui.Visible = true
		game.Lighting.CelestialBlur:Destroy()
		AdminHouse.Load.Start:Destroy()
	end
else
			
end


