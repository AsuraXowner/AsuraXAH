		UI.Frame.Frame2:TweenPosition(UDim2.new(0, 0,-1.75, 0))
		UI.Frame.Frame2:TweenSize(UDim2.new(0, 230,0, 57))
	end
end)
ColorSelect1.MouseButton1Click:Connect(function()
	SelectColorFrame="MainGui"
	preview2.BackgroundColor3 = MainGui.BackgroundColor3
	game:GetService("TweenService"):Create(ColorSelect1,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextColor3=Color3.new(0.254902, 0.45098, 0.658824)}):Play()
	game:GetService("TweenService"):Create(ColorSelect2,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextColor3=Color3.new(1, 1, 1)}):Play()
	game:GetService("TweenService"):Create(ColorSelect3,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextColor3=Color3.new(1, 1, 1)}):Play()
	game:GetService("TweenService"):Create(ColorSelect4,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextColor3=Color3.new(1, 1, 1)}):Play()
end)
