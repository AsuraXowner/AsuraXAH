				"Out",
				"Quad",
				0.4,
				false
			)
			wait(0.4)
			ListMessage2.Messages.BackgroundTimer.Others.Texts.InfoText.Text = TextNotify.Value
			ListMessage2.Messages.BackgroundTimer.BackgroundLoad:TweenSize(
				UDim2.new(0, 0,0, 62),
				"Out",
				"Quad",
				0.4,
				false
			)
			ListMessage2.Messages.BackgroundTransparency=0
			ListMessage2.Messages.BackgroundTimer.BackgroundTransparency=0
			ListMessage2.Messages.BackgroundTimer.Others.Images.ImageLabel.ImageTransparency=0
			ListMessage2.Messages.BackgroundTimer.Others.Texts.InfoText.TextTransparency=0
			ListMessage2.Messages:TweenSize(--Timer
				UDim2.new(0, 0,0, 50),
				"Out",
				"Quad",
				Time,
				false
			)
			wait(Time)
			ListMessage2.Messages.BackgroundTimer.BackgroundLoad:TweenSize(
				UDim2.new(0, -255,0, 62),
				"Out",
				"Quad",
				0.4,
				false
			)
			wait(0.4)
			ListMessage2.Messages.BackgroundTimer.BackgroundLoad:TweenSize(
				UDim2.new(0, 0,0, 62),
				"Out",
				"Quad",
				0.4,
				false
			)

			ListMessage2.Messages.BackgroundTimer.BackgroundTransparency=1
			ListMessage2.Messages.BackgroundTimer.Others.Images.ImageLabel.ImageTransparency=1
			ListMessage2.Messages.BackgroundTimer.Others.Texts.InfoText.TextTransparency=1
			wait(0.7)
			ListMessage2.Messages:Destroy()
		else
			Hidden=false
			dragging = true
			MainGui.Visible = true
			game:GetService("TweenService"):Create(Gui.Lines.Line1.LocationOfLine.Line1,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{BackgroundTransparency = 0.8}):Play()
			game:GetService("TweenService"):Create(Gui.Lines.Line1.LocationOfLine.Line4,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{BackgroundTransparency = 0.8}):Play()
			game:GetService("TweenService"):Create(Gui.Lines.Line1.LocationOfLine.Line6,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{BackgroundTransparency = 0.8}):Play()
			game:GetService("TweenService"):Create(Gui.Lines.Line2.Line2,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{BackgroundTransparency = 0.8}):Play()
			game:GetService("TweenService"):Create(Gui.Lines.Line3.LocationOfLine.Line2,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{BackgroundTransparency = 0.8}):Play()
			game:GetService("TweenService"):Create(Gui.Lines.Line3.LocationOfLine.Line3,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{BackgroundTransparency = 0.8}):Play()
			game:GetService("TweenService"):Create(Gui.Lines.Line3.LocationOfLine.Line4,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{BackgroundTransparency = 0.8}):Play()
			game:GetService("TweenService"):Create(MainGui.Parent.AdvancedFrame.MainGui,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{BackgroundTransparency = 0}):Play()
			game:GetService("TweenService"):Create(Gui.Topbar.Texts.Text,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextTransparency = 0}):Play()
			game:GetService("TweenService"):Create(Gui.Logo2,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{ImageTransparency = 0}):Play()
			game:GetService("TweenService"):Create(Gui.Logo,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{ImageTransparency = 0}):Play()
			game:GetService("TweenService"):Create(SearchBar,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{ImageTransparency = 0}):Play()
			game:GetService("TweenService"):Create(SettingsBar,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{ImageTransparency = 0}):Play()
			game:GetService("TweenService"):Create(MainGui.Parent.AdvancedFrame.MainGui,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{BackgroundColor3=Color3.fromRGB(18, 18, 18)}):Play()
			Button1Local.Visible = true
			Button2Local.Visible = true
			Button3Local.Visible = true
			Button4Local.Visible = true
			if Button1==true then
				game:GetService("TweenService"):Create(PanelGuiTransparencyValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0.2}):Play()
				game:GetService("TweenService"):Create(PanelGuiTransparencyTextValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0}):Play()
				game:GetService("TweenService"):Create(PanelGuiTransparencyBarValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0.8}):Play()
				game:GetService("TweenService"):Create(SettingsGuiTransparencyValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
				game:GetService("TweenService"):Create(SettingsGuiTransparencyTextValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
				game:GetService("TweenService"):Create(SettingsGuiTransparencyBarValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
				game:GetService("TweenService"):Create(MiscGuiTransparencyValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
				game:GetService("TweenService"):Create(MiscGuiTransparencyTextValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
				game:GetService("TweenService"):Create(MiscGuiTransparencyBarValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
			else
				if Button3==true then
					game:GetService("TweenService"):Create(PanelGuiTransparencyValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
					game:GetService("TweenService"):Create(PanelGuiTransparencyTextValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
					game:GetService("TweenService"):Create(PanelGuiTransparencyBarValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
					game:GetService("TweenService"):Create(SettingsGuiTransparencyValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
					game:GetService("TweenService"):Create(SettingsGuiTransparencyTextValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
					game:GetService("TweenService"):Create(SettingsGuiTransparencyBarValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
					game:GetService("TweenService"):Create(MiscGuiTransparencyValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0.2}):Play()
					game:GetService("TweenService"):Create(MiscGuiTransparencyTextValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0}):Play()
					game:GetService("TweenService"):Create(MiscGuiTransparencyBarValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0.8}):Play()
				else
					if Button4==true then
						game:GetService("TweenService"):Create(PanelGuiTransparencyValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
						game:GetService("TweenService"):Create(PanelGuiTransparencyTextValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
						game:GetService("TweenService"):Create(PanelGuiTransparencyBarValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
						game:GetService("TweenService"):Create(SettingsGuiTransparencyValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0.2}):Play()
						game:GetService("TweenService"):Create(SettingsGuiTransparencyTextValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0}):Play()
						game:GetService("TweenService"):Create(SettingsGuiTransparencyBarValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0.8}):Play()
						game:GetService("TweenService"):Create(MiscGuiTransparencyValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
						game:GetService("TweenService"):Create(MiscGuiTransparencyTextValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
						game:GetService("TweenService"):Create(MiscGuiTransparencyBarValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
					else
					end
				end
			end
		end
	end
end)

if game.Players.LocalPlayer.Name=="HELENERA_20" or game.Players.LocalPlayer.Name=="xuia0" then
