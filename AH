Button5Local.Button.MouseLeave:Connect(function()
	game:GetService("TweenService"):Create(Button5Local.Button.Icon,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{ImageTransparency = 0.5}):Play()
	game:GetService("TweenService"):Create(Button5Local.Button.Icon,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{ImageColor3=Color3.new(0.290196, 0.321569, 0.345098)}):Play()
	game:GetService("TweenService"):Create(Button5Local.Button.Text1,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextTransparency = 0.5}):Play()
	game:GetService("TweenService"):Create(Button5Local.Button.Text1,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextColor3=Color3.new(0.392157, 0.392157, 0.392157)}):Play()
	game:GetService("TweenService"):Create(Button5Local.Button.Text2,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextTransparency = 0.5}):Play()
	game:GetService("TweenService"):Create(Button5Local.Button.Text2,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextColor3=Color3.new(0.392157, 0.392157, 0.392157)}):Play()
	--sets other things transparency
	if Button1==true then
		game:GetService("TweenService"):Create(Button1Local.Button.Icon,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{ImageTransparency = 0}):Play()
		game:GetService("TweenService"):Create(Button1Local.Button.Icon,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{ImageColor3=LeftBarButtonsShowValue}):Play()
		game:GetService("TweenService"):Create(Button1Local.Button.Text1,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextTransparency = 0}):Play()
