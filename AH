local sp2 = Gui3.Panel.PanelButtons.ScrollingFrame.Button4.Bar.Frame.Button
local percent2 = sp2.Parent.Parent.Parent.Values.Percent
local percentText2 = sp2.Parent.Parent.Parent.Texts.Percent
local Button1Remove2 = sp2.Parent.Parent.Parent.Buttons.RemoveButton
local Button1Add2 = sp2.Parent.Parent.Parent.Buttons.AddButton
local run2 = game:GetService("RunService")

local xpos = {sp2.Position.X.Scale, sp2.Position.X.Offset}
local ypos = {sp2.Position.Y.Scale, sp2.Position.Y.Offset}

run2.RenderStepped:connect(function()
	sp2.Position = UDim2.new(sp2.Position.X.Scale, sp2.Position.X.Offset , ypos[1], ypos[2])
	if sp2.Position.X.Offset >= sp2.Parent.Parent.Size.X.Offset - sp2.Size.X.Offset then
		sp2.Position = UDim2.new(sp2.Position.X.Scale, (sp2.Parent.Parent.Size.X.Offset - sp2.Size.X.Offset) , ypos[1], ypos[2])
	elseif sp2.Position.X.Offset <= 0 then
		sp2.Position = UDim2.new(xpos[1], xpos[2] , ypos[1], ypos[2])
	end

	percent2.Value = math.floor((sp2.Position.X.Offset / (sp2.Parent.Parent.Size.X.Offset - sp2.Size.X.Offset)) * 530)
	percentText2.Text = math.floor((sp2.Position.X.Offset / (sp2.Parent.Parent.Size.X.Offset - sp2.Size.X.Offset)) * 530).."%"
end)
Button1Remove2.MouseButton1Click:Connect(function()
	sp2:TweenPosition(UDim2.new(0, 0,0, 9))
	Sound:Play()
end)
Button1Add2.MouseButton1Click:Connect(function()
	sp2:TweenPosition(UDim2.new(0, 127,0, 9))
	Sound:Play()
end)
sp2.Parent.Parent.MouseEnter:Connect(function()
	game:GetService("TweenService"):Create(sp2.Parent.Parent.Parent.Texts.Text,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextColor3=Color3.new(0.729412, 0.729412, 0.729412)}):Play()
	game:GetService("TweenService"):Create(percentText2,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextColor3=Color3.new(0.729412, 0.729412, 0.729412)}):Play()
end)
sp2.Parent.Parent.MouseLeave:Connect(function()
	game:GetService("TweenService"):Create(percentText2,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextColor3=Color3.new(0.415686, 0.415686, 0.415686)}):Play()
	game:GetService("TweenService"):Create(sp2.Parent.Parent.Parent.Texts.Text,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextColor3=Color3.new(0.415686, 0.415686, 0.415686)}):Play()
