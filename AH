		game:GetService("TweenService"):Create(Button1Panel.Button,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{BackgroundTransparency = 0}):Play()
		game:GetService("TweenService"):Create(Button1Panel.Button.Icon,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{ImageTransparency = 0}):Play()
	end
end)
Button1Panel.Button.MouseLeave:Connect(function()
	if Button1PanelValue==false then
		game:GetService("TweenService"):Create(Button1Panel.Text,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextTransparency = 0.5}):Play()
		game:GetService("TweenService"):Create(Button1Panel,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{BackgroundTransparency = 0.6}):Play()
		game:GetService("TweenService"):Create(Button1Panel.Button,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{BackgroundTransparency = 0.6}):Play()
		game:GetService("TweenService"):Create(Button1Panel.Text,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextColor3=Color3.new(0.254902, 0.254902, 0.254902)}):Play()
	else
		game:GetService("TweenService"):Create(Button1Panel.Text,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextTransparency = 0.5}):Play()
		game:GetService("TweenService"):Create(Button1Panel,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{BackgroundTransparency = 0.6}):Play()
		game:GetService("TweenService"):Create(Button1Panel.Button,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{BackgroundTransparency = 0.6}):Play()
		game:GetService("TweenService"):Create(Button1Panel.Button.Icon,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{ImageTransparency = 0.6}):Play()
	end
end)
Button1Panel.Button.MouseButton1Click:Connect(function()
	if Button1PanelValue==false then --enable
		Button1PanelValue=true
		local Mouse = game:GetService("Players").LocalPlayer:GetMouse()
		local Flyspeed = 10
		local FlySpeed = Flyspeed

		_G.FLYING = true--actual fr
		function sFLY()
			repeat wait() until game:GetService("Players").LocalPlayer and game:GetService("Players").LocalPlayer.Character and game:GetService("Players").LocalPlayer.Character:FindFirstChild('HumanoidRootPart') and game:GetService("Players").LocalPlayer.Character:FindFirstChild('Humanoid')
			repeat wait() until Mouse

			local T = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart
			local CONTROL = {F = 0, B = 0, L = 0, R = 0}
			local lCONTROL = {F = 0, B = 0, L = 0, R = 0}
			local SPEED = 0

			local function FLY()
				_G.FLYING = true
				local BG = Instance.new('BodyGyro', T)
				local BV = Instance.new('BodyVelocity', T)
				BG.P = 9e4
				BG.maxTorque = Vector3.new(9e9, 9e9, 9e9)
				BG.cframe = T.CFrame
				BV.velocity = Vector3.new(0, 0, 0)
				BV.maxForce = Vector3.new(9e9, 9e9, 9e9)
				spawn(function()
					repeat wait()
						game:GetService("Players").LocalPlayer.Character.Humanoid.PlatformStand = true
						if CONTROL.L + CONTROL.R ~= 0 or CONTROL.F + CONTROL.B ~= 0 then
							SPEED = 50
						elseif not (CONTROL.L + CONTROL.R ~= 0 or CONTROL.F + CONTROL.B ~= 0) and SPEED ~= 0 then
							SPEED = 0
						end
						if (CONTROL.L + CONTROL.R) ~= 0 or (CONTROL.F + CONTROL.B) ~= 0 then
							BV.velocity = ((workspace.CurrentCamera.CoordinateFrame.lookVector * (CONTROL.F + CONTROL.B)) + ((workspace.CurrentCamera.CoordinateFrame * CFrame.new(CONTROL.L + CONTROL.R, (CONTROL.F + CONTROL.B) * 0.2, 0).p) - workspace.CurrentCamera.CoordinateFrame.p)) * SPEED
							lCONTROL = {F = CONTROL.F, B = CONTROL.B, L = CONTROL.L, R = CONTROL.R}
						elseif (CONTROL.L + CONTROL.R) == 0 and (CONTROL.F + CONTROL.B) == 0 and SPEED ~= 0 then
							BV.velocity = ((workspace.CurrentCamera.CoordinateFrame.lookVector * (lCONTROL.F + lCONTROL.B)) + ((workspace.CurrentCamera.CoordinateFrame * CFrame.new(lCONTROL.L + lCONTROL.R, (lCONTROL.F + lCONTROL.B) * 0.2, 0).p) - workspace.CurrentCamera.CoordinateFrame.p)) * SPEED
						else
							BV.velocity = Vector3.new(0, 0, 0)
						end
						BG.cframe = workspace.CurrentCamera.CoordinateFrame
					until not _G.FLYING
					CONTROL = {F = 0, B = 0, L = 0, R = 0}
					lCONTROL = {F = 0, B = 0, L = 0, R = 0}
					SPEED = 0
					BG:destroy()
					BV:destroy()
					game:GetService("Players").LocalPlayer.Character.Humanoid.PlatformStand = false
					NOFLY()
				end)
			end



			Mouse.KeyDown:connect(function(KEY)
				if KEY:lower() == 'w' then
					CONTROL.F = FlySpeed
				elseif KEY:lower() == 's' then
					CONTROL.B = -FlySpeed
				elseif KEY:lower() == 'a' then
					CONTROL.L = -FlySpeed 
				elseif KEY:lower() == 'd' then 
					CONTROL.R = FlySpeed
				end
			end)
			Mouse.KeyUp:connect(function(KEY)
				if KEY:lower() == 'w' then
					CONTROL.F = 0
				elseif KEY:lower() == 's' then
					CONTROL.B = 0
				elseif KEY:lower() == 'a' then
					CONTROL.L = 0
				elseif KEY:lower() == 'd' then
					CONTROL.R = 0
				end
			end)
			FLY()
		end
		spawn(function()while wait() do
				if not _G.FLYING then
					NOFLY()
				end
			end end)

		function NOFLY()
			_G.FLYING = false
			game:GetService("Players").LocalPlayer.Character.Humanoid.PlatformStand = false
			for i,v in pairs(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart:GetChildren()) do
				if v.ClassName == "BodyGyro" or v.ClassName == "BodyVelocity" then
					v:Destroy()
				end
			end
			
		end

		sFLY()
		--value
		game:GetService("TweenService"):Create(Button1Panel.Text,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextTransparency = 0}):Play()
		game:GetService("TweenService"):Create(Button1Panel,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{BackgroundTransparency = 0}):Play()
		game:GetService("TweenService"):Create(Button1Panel.Button,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{BackgroundTransparency = 0}):Play()
		game:GetService("TweenService"):Create(Button1Panel.Button.Icon,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{ImageTransparency = 0}):Play()
		game:GetService("TweenService"):Create(Button1Panel.Text,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextColor3=Color3.new(0.454902, 0.494118, 0.6)}):Play()
		game:GetService("TweenService"):Create(Button1Panel.Button,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextColor3=Color3.new(0.454902, 0.494118, 0.6)}):Play()
		game:GetService("TweenService"):Create(Button1Panel.Button.Icon,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{ImageColor3=Color3.new(0.454902, 0.494118, 0.6)}):Play()
		Button1Panel.Button.Icon.Visible=true
	else --close
		Button1Panel.Button.Icon.Visible=false
		Button1PanelValue=false
		game:GetService("TweenService"):Create(Button1Panel.Button.Icon,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{ImageTransparency = 0.6}):Play()
		game:GetService("TweenService"):Create(Button1Panel.Text,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextColor3=Color3.new(0.254902, 0.254902, 0.254902)}):Play()
		game:GetService("TweenService"):Create(Button1Panel.Button,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextColor3=Color3.new(0.254902, 0.254902, 0.254902)}):Play()
		game:GetService("TweenService"):Create(Button1Panel.Button.Icon,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{ImageColor3=Color3.new(0.254902, 0.254902, 0.254902)}):Play()
		game.Players.LocalPlayer.Character.Humanoid.Health = 0
		wait(9)
		Gui.Logo.ImageColor3 = Color3.fromRGB(65, 115, 168)
		Gui.Lines.Line1.LocationOfLine.Line1.BackgroundColor3 = Color3.fromRGB(47, 47, 47)
		Gui.Lines.Line1.LocationOfLine.Line4.BackgroundColor3 = Color3.fromRGB(47, 47, 47)
		Gui.Lines.Line1.LocationOfLine.Line6.BackgroundColor3 = Color3.fromRGB(47, 47, 47)
		Gui.Lines.Line2.Line2.BackgroundColor3 = Color3.fromRGB(47, 47, 47)
		Gui.Lines.Line3.LocationOfLine.Line2.BackgroundColor3 = Color3.fromRGB(47, 47, 47)
		Gui.Lines.Line3.LocationOfLine.Line3.BackgroundColor3 = Color3.fromRGB(47, 47, 47)
		Gui.Lines.Line3.LocationOfLine.Line4.BackgroundColor3 = Color3.fromRGB(47, 47, 47)
		--LoadingImage.ImageColor3 = Color3.fromRGB(255, 255, 255)
		Gui.Logo2.ImageColor3 = Color3.fromRGB(65, 115, 168)
		Gui.SideBars.Background.Frame.BackgroundColor3 = Color3.fromRGB(18, 18, 18)
		MainGui.BackgroundColor3 = Color3.fromRGB(18, 18, 18)
		MainGui.Transparency=0
		Gui.SideBars.Background.Frame.Transparency=0
	end
end)

Button2Panel.Text.MouseEnter:Connect(function()
	if Button2PanelValue==false then
		game:GetService("TweenService"):Create(Button2Panel.Text,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextTransparency = 0}):Play()
		game:GetService("TweenService"):Create(Button2Panel,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{BackgroundTransparency = 0}):Play()
		game:GetService("TweenService"):Create(Button2Panel.ImageButton,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{ImageTransparency = 0}):Play()
		game:GetService("TweenService"):Create(Button2Panel.Text,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextColor3=Color3.new(0.415686, 0.415686, 0.415686)}):Play()
	else
		game:GetService("TweenService"):Create(Button2Panel.Text,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextTransparency = 0}):Play()
		game:GetService("TweenService"):Create(Button2Panel,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{BackgroundTransparency = 0}):Play()
		game:GetService("TweenService"):Create(Button2Panel.AdminPadsGui,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{BackgroundTransparency = 0}):Play()
		game:GetService("TweenService"):Create(Button2Panel.ImageButton,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{ImageTransparency = 0}):Play()
		Button2Panel.AdminPadsGui:TweenPosition(UDim2.new(-0.007, 0,1.071, 0))
		Button2Panel.AdminPadsGui:TweenSize(UDim2.new(0, 149,0, 100))
	end
end)
