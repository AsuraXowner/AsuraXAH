Button1Settings.Button.MouseButton1Click:Connect(function()
	if Button1SettingsValue==false then --enable
		Button1SettingsValue=true
		RainbowModeValue=true
		--value
		game:GetService("TweenService"):Create(Button1Settings.Text,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextTransparency = 0}):Play()
		game:GetService("TweenService"):Create(Button1Settings,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{BackgroundTransparency = 0}):Play()
		game:GetService("TweenService"):Create(Button1Settings.Button,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{BackgroundTransparency = 0}):Play()
		game:GetService("TweenService"):Create(Button1Settings.Button.Icon,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{ImageTransparency = 0}):Play()
		game:GetService("TweenService"):Create(Button1Settings.Text,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextColor3=Color3.new(0.454902, 0.494118, 0.6)}):Play()
		game:GetService("TweenService"):Create(Button1Settings.Button,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextColor3=Color3.new(0.454902, 0.494118, 0.6)}):Play()
		game:GetService("TweenService"):Create(Button1Settings.Button.Icon,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{ImageColor3=Color3.new(0.454902, 0.494118, 0.6)}):Play()
		Button1Settings.Button.Icon.Visible=true
	else --close
		Button1Settings.Button.Icon.Visible=false
		Button1SettingsValue=false
		RainbowModeValue=false
		game:GetService("TweenService"):Create(Button1Settings.Button.Icon,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{ImageTransparency = 0.6}):Play()
		game:GetService("TweenService"):Create(Button1Settings.Text,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextColor3=Color3.new(0.192157, 0.192157, 0.192157)}):Play()
		game:GetService("TweenService"):Create(Button1Settings.Button,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextColor3=Color3.new(0.211765, 0.211765, 0.211765)}):Play()
		game:GetService("TweenService"):Create(Button1Settings.Button.Icon,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{ImageColor3=Color3.new(0.223529, 0.223529, 0.223529)}):Play()
	end
end)
