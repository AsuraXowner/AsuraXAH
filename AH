-- Scripts:


local function LFDQZD_fake_script() -- clockMain
	local script = Instance.new('LocalScript', Instances)
	
	local LoadingGui = AdminHouse.Load.Start
	local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Linear, Enum.EasingDirection.In)
	local TweenService = game:GetService("TweenService")
	while wait(0.01) do
	if AdminHouse.Main.Instances.LoadValue.Value==true then
		LoadingGui.LoadingScreen.SpinningCircle.Rotation = 0
		TweenService:Create(LoadingGui.LoadingScreen.SpinningCircle, tweenInfo, {Rotation = 360}):Play()
		wait(1)
	else
		end
	end
end

local function LFDQFD_fake_script() -- LoadSpinning
	local script = Instance.new('LocalScript', Instances)
	
	local cam = game.Workspace.CurrentCamera
	repeat wait() until (Instances.Clock.Value)
	local clock = Instances.Clock.Value
	clock.Parent = cam
	clock:MakeJoints()
	clock.PrimaryPart = clock.Pole
	clock:MoveTo(clock.Pos.Value)

	function start()
		while (true) do
			local t = tick()
			local sec = (t%60)
			local min = ((t/60)%60)
			local hour = ((t/3600)%24)
			hour = (hour > 12 and (hour-12) or hour)
			local hourAngle = (360*(hour/12))+90
			local minuteAngle = (360*(min/60))+90
			local secondAngle = (360*(sec/60))+90
			clock.HourHand.CFrame = (clock.Center.CFrame*CFrame.Angles(0,math.rad(hourAngle),0))
			clock.MinuteHand.CFrame = (clock.Center.CFrame*CFrame.Angles(0,math.rad(minuteAngle),0))
			clock.SecondHand.CFrame = (clock.Center.CFrame*CFrame.Angles(0,math.rad(secondAngle),0))
			wait()
		end
	end

	start()

end

coroutine.wrap(LFDQZD_fake_script)()

local function PBYDSLC_fake_script() -- AdminHouse.Messages.MessageScript 
	local script = Instance.new('LocalScript', AdminHouse.Messages)

	local Message1 = script.Parent
	script.Parent.Parent:WaitForChild("UIListLayout")
	local Others = script.Parent.BackgroundTimer.Others
	local Timer = script.Parent.BackgroundTimer
	local Load = Timer.BackgroundLoad
	local MessageText = Others.Texts.InfoText
	local MessageImage = Others.Images.ImageLabel
	local Time = 4
	local Text1 = script.TextNotify.Value
	local Color = script.NotifyColor.Value
	local ImageID = script.ImageID.Value
	local ImageIDScript = "rbxassetid://12988744896"

	Message1.BackgroundColor3 = Color
	Load.BackgroundColor3 = Color
	MessageImage.ImageColor3 = Color
	MessageText.TextColor3 = Color
	MessageImage.Image = ImageID
	Load:TweenSize(
		UDim2.new(0, -255,0, 62),
		"Out",
		"Quad",
		0.4,
		false
	)
	wait(0.4)
	MessageText.Text = Text1
	Load:TweenSize(
		UDim2.new(0, 0,0, 62),
		"Out",
		"Quad",
		0.4,
		false
	)
	Message1.BackgroundTransparency=0
	Timer.BackgroundTransparency=0
	MessageImage.ImageTransparency=0
	MessageText.TextTransparency=0
	Message1:TweenSize(--Timer
		UDim2.new(0, 0,0, 50),
		"Out",
		"Quad",
		Time,
		false
	)
	wait(Time)
	Load:TweenSize(
		UDim2.new(0, -255,0, 62),
		"Out",
		"Quad",
		0.4,
		false
	)
	wait(0.4)
	Load:TweenSize(
		UDim2.new(0, 0,0, 62),
		"Out",
		"Quad",
		0.4,
		false
	)

	Timer.BackgroundTransparency=1
	MessageImage.ImageTransparency=1
	MessageText.TextTransparency=1
	wait(0.7)
	script.Parent:Destroy()
end
coroutine.wrap(PBYDSLC_fake_script)()

