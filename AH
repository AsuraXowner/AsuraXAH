MainGui.Parent.AdvancedFrame.MainGui.Button.MouseButton1Click:Connect(function()
	if ClockGui==false then
		game:GetService("TweenService"):Create(PanelGuiTransparencyValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
		game:GetService("TweenService"):Create(PanelGuiTransparencyTextValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
		game:GetService("TweenService"):Create(PanelGuiTransparencyBarValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
		game:GetService("TweenService"):Create(SettingsGuiTransparencyValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
		game:GetService("TweenService"):Create(SettingsGuiTransparencyTextValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
