--Misc--------------------------------------------Locals--------------------------------------------------
local MiscButton1 = Gui.Menu.Misc.MiscButtons.ScrollingFrame.Button1


---------------Buttons------------------------------
MiscButton1.MouseEnter:Connect(function()
	if Button1CheckboxMisc==false then
		MiscButton1.Main.Button1:TweenPosition(UDim2.new(0.849, -15,0.397, -10))
		game:GetService("TweenService"):Create(MiscButton1.Main,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundColor3=Color3.fromRGB(65, 115, 168)}):Play()
		game:GetService("TweenService"):Create(MiscButton1.Main.Button1,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundColor3=Color3.fromRGB(65, 115, 168)}):Play()
	end
end)
MiscButton1.MouseLeave:Connect(function()
	if Button1CheckboxMisc==false then
		MiscButton1.Main.Button1:TweenPosition(UDim2.new(0.349, -15,0.397, -10))
		game:GetService("TweenService"):Create(MiscButton1.Main,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundColor3=Color3.fromRGB(72, 72, 72)}):Play()
		game:GetService("TweenService"):Create(MiscButton1.Main.Button1,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundColor3=Color3.fromRGB(72, 72, 72)}):Play()
	end
end)
MiscButton1.MouseLeave:Connect(function()
	if Button1CheckboxMisc==true then
		MiscButton1.Main.Button1:TweenPosition(UDim2.new(0.849, -15,0.397, -10))
		game:GetService("TweenService"):Create(MiscButton1.Main,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundColor3=Color3.fromRGB(65, 115, 168)}):Play()
		game:GetService("TweenService"):Create(MiscButton1.Main.Button1,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundColor3=Color3.fromRGB(65, 115, 168)}):Play()
	end
end)
MiscButton1.MouseEnter:Connect(function()
	if Button1CheckboxMisc==true then
		MiscButton1.Main.Button1:TweenPosition(UDim2.new(0.349, -15,0.397, -10))
		game:GetService("TweenService"):Create(MiscButton1.Main,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundColor3=Color3.fromRGB(72, 72, 72)}):Play()
		game:GetService("TweenService"):Create(MiscButton1.Main.Button1,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundColor3=Color3.fromRGB(72, 72, 72)}):Play()
	end
end)
MiscButton1.Main.MouseButton1Click:Connect(function()--backgroundDetector
	if Button1CheckboxMisc==false then--if button is not enabled
		Button1CheckboxMisc=true
		MiscButton1.Main.Button1:TweenPosition(UDim2.new(0.849, -15,0.397, -10))
		game:GetService("TweenService"):Create(MiscButton1.Main,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundColor3=Color3.fromRGB(65, 115, 168)}):Play()
		game:GetService("TweenService"):Create(MiscButton1.Main.Button1,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundColor3=Color3.fromRGB(65, 115, 168)}):Play()
	else--if button is active
		Button1CheckboxMisc=false
		MiscButton1.Main.Button1:TweenPosition(UDim2.new(0.349, -15,0.397, -10))
		game:GetService("TweenService"):Create(MiscButton1.Main,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundColor3=Color3.fromRGB(72, 72, 72)}):Play()
		game:GetService("TweenService"):Create(MiscButton1.Main.Button1,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundColor3=Color3.fromRGB(72, 72, 72)}):Play()
	end	
end)
MiscButton1.Main.Button1.Button2.MouseButton1Click:Connect(function()--ButtonDetector
	if Button1CheckboxMisc==false then--if button is not enabled
		Button1CheckboxMisc=true
		MiscButton1.Main.Button1:TweenPosition(UDim2.new(0.849, -15,0.397, -10))
		game:GetService("TweenService"):Create(MiscButton1.Main,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundColor3=Color3.fromRGB(65, 115, 168)}):Play()
		game:GetService("TweenService"):Create(MiscButton1.Main.Button1,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundColor3=Color3.fromRGB(65, 115, 168)}):Play()
	else--if button is active
		Button1CheckboxMisc=false
		MiscButton1.Main.Button1:TweenPosition(UDim2.new(0.349, -15,0.397, -10))
		game:GetService("TweenService"):Create(MiscButton1.Main,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundColor3=Color3.fromRGB(72, 72, 72)}):Play()
		game:GetService("TweenService"):Create(MiscButton1.Main.Button1,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundColor3=Color3.fromRGB(72, 72, 72)}):Play()
	end	
end)
--and slides

local Gui3 = AdminHouse.Main.MainGui.Menu

local sp1 = Gui3.Panel.PanelButtons.ScrollingFrame.Button3.Bar.Frame.Button
local percent1 = sp1.Parent.Parent.Parent.Values.Percent
local percentText1 = sp1.Parent.Parent.Parent.Texts.Percent
local Button1Remove1 = sp1.Parent.Parent.Parent.Buttons.RemoveButton
local Button1Add1 = sp1.Parent.Parent.Parent.Buttons.AddButton
local run1 = game:GetService("RunService")

local xpos = {sp1.Position.X.Scale, sp1.Position.X.Offset}
local ypos = {sp1.Position.Y.Scale, sp1.Position.Y.Offset}

run1.RenderStepped:connect(function()
	sp1.Position = UDim2.new(sp1.Position.X.Scale, sp1.Position.X.Offset , ypos[1], ypos[2])
	if sp1.Position.X.Offset >= sp1.Parent.Parent.Size.X.Offset - sp1.Size.X.Offset then
		sp1.Position = UDim2.new(sp1.Position.X.Scale, (sp1.Parent.Parent.Size.X.Offset - sp1.Size.X.Offset) , ypos[1], ypos[2])
	elseif sp1.Position.X.Offset <= 0 then
		sp1.Position = UDim2.new(xpos[1], xpos[2] , ypos[1], ypos[2])
	end

	percent1.Value = math.floor((sp1.Position.X.Offset / (sp1.Parent.Parent.Size.X.Offset - sp1.Size.X.Offset)) * 525)
	percentText1.Text = math.floor((sp1.Position.X.Offset / (sp1.Parent.Parent.Size.X.Offset - sp1.Size.X.Offset)) * 525).."%"
end)
Button1Remove1.MouseButton1Click:Connect(function()
	sp1:TweenPosition(UDim2.new(0, 0,0, 9))
	Sound:Play()
end)
Button1Add1.MouseButton1Click:Connect(function()
	sp1:TweenPosition(UDim2.new(0, 127,0, 9))
	Sound:Play()
end)
sp1.Parent.Parent.MouseEnter:Connect(function()
	game:GetService("TweenService"):Create(sp1.Parent.Parent.Parent.Texts.Text,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextColor3=Color3.new(0.729412, 0.729412, 0.729412)}):Play()
	game:GetService("TweenService"):Create(percentText1,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextColor3=Color3.new(0.729412, 0.729412, 0.729412)}):Play()
