		game:GetService("TweenService"):Create(SettingsGuiTransparencyValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
		game:GetService("TweenService"):Create(SettingsGuiTransparencyTextValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
		game:GetService("TweenService"):Create(SettingsGuiTransparencyBarValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
		game:GetService("TweenService"):Create(MiscGuiTransparencyValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
		game:GetService("TweenService"):Create(MiscGuiTransparencyTextValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
		game:GetService("TweenService"):Create(MiscGuiTransparencyBarValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
		game:GetService("TweenService"):Create(Gui.Lines.Line1.LocationOfLine.Line1,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
		game:GetService("TweenService"):Create(Gui.Lines.Line1.LocationOfLine.Line4,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
		game:GetService("TweenService"):Create(Gui.Lines.Line1.LocationOfLine.Line6,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
		game:GetService("TweenService"):Create(Gui.Lines.Line2.Line2,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
		game:GetService("TweenService"):Create(Gui.Lines.Line3.LocationOfLine.Line2,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
		game:GetService("TweenService"):Create(Gui.Lines.Line3.LocationOfLine.Line3,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
		game:GetService("TweenService"):Create(Gui.Lines.Line3.LocationOfLine.Line4,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
		game:GetService("TweenService"):Create(Gui.Topbar.Texts.Text,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextTransparency = 1}):Play()
		game:GetService("TweenService"):Create(Gui.Logo2,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{ImageTransparency = 1}):Play()
		game:GetService("TweenService"):Create(Gui.Logo,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{ImageTransparency = 1}):Play()
		game:GetService("TweenService"):Create(SearchBar,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{ImageTransparency = 1}):Play()
		game:GetService("TweenService"):Create(SettingsBar,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{ImageTransparency = 1}):Play()
		wait(1)
		Button1Local.Visible = false
		Button2Local.Visible = false
		Button3Local.Visible = false
		Button4Local.Visible = false
		dragging = false
		MainGui.Visible = false
			Hidden=true
			Sound3:Play()
			TextNotify.Value="Interface hidden press again m to open gui"
			ImageID.Value="rbxassetid://9405926389"
			NotifyColor.Value=Color3.new(1, 1, 1)
			AdminHouse.Messages:Clone().Parent = ListMessage2
			ListMessage2.Messages.BackgroundTimer.Others.Texts.InfoText:TweenSize(UDim2.new(0, 160,0, 34))
			ListMessage2.Messages.BackgroundTimer.Others.Texts.InfoText.TextScaled=true
			ListMessage2.Messages.BackgroundColor3 = NotifyColor.Value
			ListMessage2.Messages.BackgroundTimer.BackgroundLoad.BackgroundColor3 = NotifyColor.Value
			ListMessage2.Messages.BackgroundTimer.Others.Images.ImageLabel.ImageColor3 = NotifyColor.Value
			ListMessage2.Messages.BackgroundTimer.Others.Texts.InfoText.TextColor3 = NotifyColor.Value
			ListMessage2.Messages.BackgroundTimer.Others.Images.ImageLabel.Image = ImageID.Value
