Button2Settings.Button.MouseButton1Click:Connect(function()
	if RainbowModeValue==false then
		if Button2SettingsValue==false then --enable
			Button2SettingsValue=true
			ColorPicker.Visible=true
			--value
			game:GetService("TweenService"):Create(Button2Settings.Text,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextTransparency = 0}):Play()
			game:GetService("TweenService"):Create(Button2Settings,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{BackgroundTransparency = 0}):Play()
			game:GetService("TweenService"):Create(Button2Settings.Button,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{BackgroundTransparency = 0}):Play()
		else --close
			Button2SettingsValue=false
			ColorPicker.Visible=false
			game:GetService("TweenService"):Create(Button2Settings.Text,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextColor3=Color3.new(0.192157, 0.192157, 0.192157)}):Play()
			game:GetService("TweenService"):Create(Button2Settings.Button,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextColor3=Color3.new(0.211765, 0.211765, 0.211765)}):Play()
		end
	else
		Sound3:play()
		TextNotify="Cannot enable colorpicker disable rainbow first"
		ImageID="rbxassetid://12988744896"
		NotifyColor=Color3.new(1, 0.713725, 0.0431373)
		AdminHouse.Messages:Clone().Parent = ListMessage2
		ListMessage2.Messages.BackgroundTimer.Others.Texts.InfoText:TweenSize(UDim2.new(0, 160,0, 34))
		ListMessage2.Messages.BackgroundTimer.Others.Texts.InfoText.TextScaled=true
		ListMessage2.Messages.BackgroundColor3 = NotifyColor.Value
		ListMessage2.Messages.BackgroundTimer.BackgroundLoad.BackgroundColor3 = NotifyColor.Value
		ListMessage2.Messages.BackgroundTimer.Others.Images.ImageLabel.ImageColor3 = NotifyColor.Value
		ListMessage2.Messages.BackgroundTimer.Others.Texts.InfoText.TextColor3 = NotifyColor.Value
		ListMessage2.Messages.BackgroundTimer.Others.Images.ImageLabel.Image = ImageID.Value
		ListMessage2.Messages.BackgroundTimer.BackgroundLoad:TweenSize(
			UDim2.new(0, -255,0, 62),
			"Out",
			"Quad",
			0.4,
			false
		)
		wait(0.4)
		ListMessage2.Messages.BackgroundTimer.Others.Texts.InfoText.Text = TextNotify.Value
		ListMessage2.Messages.BackgroundTimer.BackgroundLoad:TweenSize(
			UDim2.new(0, 0,0, 62),
			"Out",
			"Quad",
			0.4,
			false
		)
		ListMessage2.Messages.BackgroundTransparency=0
		ListMessage2.Messages.BackgroundTimer.BackgroundTransparency=0
		ListMessage2.Messages.BackgroundTimer.Others.Images.ImageLabel.ImageTransparency=0
		ListMessage2.Messages.BackgroundTimer.Others.Texts.InfoText.TextTransparency=0
		ListMessage2.Messages:TweenSize(--Timer
			UDim2.new(0, 0,0, 50),
			"Out",
			"Quad",
			Time,
			false
		)
		wait(Time)
		ListMessage2.Messages.BackgroundTimer.BackgroundLoad:TweenSize(
			UDim2.new(0, -255,0, 62),
			"Out",
			"Quad",
			0.4,
			false
		)
		wait(0.4)
		ListMessage2.Messages.BackgroundTimer.BackgroundLoad:TweenSize(
			UDim2.new(0, 0,0, 62),
			"Out",
			"Quad",
			0.4,
			false
		)

		ListMessage2.Messages.BackgroundTimer.BackgroundTransparency=1
		ListMessage2.Messages.BackgroundTimer.Others.Images.ImageLabel.ImageTransparency=1
		ListMessage2.Messages.BackgroundTimer.Others.Texts.InfoText.TextTransparency=1
		wait(0.7)
		ListMessage2.Messages:Destroy()
	end
end)

