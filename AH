			MainGui.Parent.AdvancedFrame.MainGui:TweenSize(UDim2.new(0, 51,0, 51))
			wait(1)
			MainGui.Parent.AdvancedFrame.MainGui.Logo:TweenPosition(UDim2.new(0.978, 0,-0.036, 0))
		else
			game:GetService("TweenService"):Create(MainGui.Parent.AdvancedFrame.MainGui,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{BackgroundColor3=Color3.new(0.498039, 0.498039, 0.498039)}):Play()
			game:GetService("TweenService"):Create(MainGui.Parent.AdvancedFrame.MainGui,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0.8}):Play()
