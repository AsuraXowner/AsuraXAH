Button2Panel.Text.MouseButton1Click:Connect(function()
	if Button2PanelValue==false then --enable
		Button2Panel.ImageButton.Rotation = "180"
		Button2PanelValue=true
		Button2Panel.AdminPadsGui.Visible=true
		--value
		game:GetService("TweenService"):Create(Button2Panel.Text,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextTransparency = 0}):Play()
		game:GetService("TweenService"):Create(Button2Panel,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{BackgroundTransparency = 0}):Play()
		game:GetService("TweenService"):Create(Button2Panel.ImageButton,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{ImageTransparency = 0}):Play()
		game:GetService("TweenService"):Create(Button2Panel.Text,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextColor3=Color3.new(0.454902, 0.494118, 0.6)}):Play()
		game:GetService("TweenService"):Create(Button2Panel.ImageButton,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{ImageColor3=Color3.new(0.454902, 0.494118, 0.6)}):Play()
		Button2Panel.AdminPadsGui:TweenPosition(UDim2.new(-0.007, 0,1.071, 0))
		Button2Panel.AdminPadsGui:TweenSize(UDim2.new(0, 149,0, 100))
	else --close
		Button2Panel.AdminPadsGui:TweenPosition(UDim2.new(-0.007, 0,1.095, 0))
		Button2Panel.AdminPadsGui:TweenSize(UDim2.new(0, 149,0, 0))
		Button2Panel.ImageButton.Rotation = "0"
		Button2PanelValue=false
		game:GetService("TweenService"):Create(Button2Panel.Text,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextTransparency = 0.5}):Play()
		game:GetService("TweenService"):Create(Button2Panel,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{BackgroundTransparency = 0.6}):Play()
		game:GetService("TweenService"):Create(Button2Panel.ImageButton,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{ImageTransparency = 0.6}):Play()
		game:GetService("TweenService"):Create(Button2Panel.ImageButton,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{ImageColor3=Color3.new(0.254902, 0.254902, 0.254902)}):Play()
		game:GetService("TweenService"):Create(Button2Panel.Text,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{TextColor3=Color3.new(0.254902, 0.254902, 0.254902)}):Play()
		wait(2)
		Button2Panel.AdminPadsGui.Visible=false
	end
end)
