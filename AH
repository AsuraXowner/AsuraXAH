	wait(2)
	Gui.SideBars.Background.Frame.Parent.Parent.Texts.Text.Visible = true
	wait(10)
	Gui.SideBars.Background.Frame.Parent.Parent.Texts.Text.Visible = false
	Gui.SideBars.Background.LeftBar:TweenSize(UDim2.new(0, 13,0, 337))
	Gui.SideBars.Background.LeftBar:TweenPosition(UDim2.new(0.975, 0,0.159, 0))
	wait(1)
