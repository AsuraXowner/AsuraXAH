SearchBar.MouseButton1Click:Connect(function()--works but not does search
	if SearchbarValue==false then
		SearchbarValue=true
		SearchTextbox.Text=""
		game:GetService("TweenService"):Create(SearchBar,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{ImageTransparency = 0}):Play()
		game:GetService("TweenService"):Create(SettingsBar,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{ImageTransparency = 1}):Play()
		game:GetService("TweenService"):Create(SearchBar,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{ImageColor3=Color3.new(0.54902, 0.54902, 0.54902)}):Play()
		SearchBar:TweenPosition(UDim2.new(0.703, 0,0.04, 0))
		SettingsBar.Visible = false
		wait(1)
		SearchFrame.Visible=true
		SearchFrame:TweenSize(UDim2.new(0, 130,0, 29))
		if Button1==true then
			game:GetService("TweenService"):Create(PanelGuiTransparencyValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0.8}):Play()
			game:GetService("TweenService"):Create(PanelGuiTransparencyTextValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0.8}):Play()
			game:GetService("TweenService"):Create(PanelGuiTransparencyBarValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0.8}):Play()
		else
			if Button4==true then
				game:GetService("TweenService"):Create(SettingsGuiTransparencyValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0.8}):Play()
				game:GetService("TweenService"):Create(SettingsGuiTransparencyTextValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0.8}):Play()
				game:GetService("TweenService"):Create(SettingsGuiTransparencyBarValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0.8}):Play()
			else
				if Button3==true then
					game:GetService("TweenService"):Create(MiscGuiTransparencyValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0.8}):Play()
					game:GetService("TweenService"):Create(MiscGuiTransparencyTextValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0.8}):Play()
					game:GetService("TweenService"):Create(MiscGuiTransparencyBarValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0.8}):Play()
					wait(1)
				end
			end
		end
	else
		SearchbarValue=false
		SearchFrame:TweenSize(UDim2.new(0, 0,0, 29))
		SettingsBar.Visible = true
		if Button1==true then
			game:GetService("TweenService"):Create(PanelGuiTransparencyValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0.2}):Play()
			game:GetService("TweenService"):Create(PanelGuiTransparencyTextValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0}):Play()
			game:GetService("TweenService"):Create(PanelGuiTransparencyBarValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0.8}):Play()
		else
			if Button4==true then
				game:GetService("TweenService"):Create(SettingsGuiTransparencyValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0.2}):Play()
				game:GetService("TweenService"):Create(SettingsGuiTransparencyTextValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0}):Play()
				game:GetService("TweenService"):Create(SettingsGuiTransparencyBarValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0.8}):Play()
			else
				if Button3==true then
					game:GetService("TweenService"):Create(MiscGuiTransparencyValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0.2}):Play()
					game:GetService("TweenService"):Create(MiscGuiTransparencyTextValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0}):Play()
					game:GetService("TweenService"):Create(MiscGuiTransparencyBarValue.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0.8}):Play()
				end
			end
		end
		wait(1)
		SearchFrame.Visible=false
		if Button4==false then
			SearchBar:TweenPosition(UDim2.new(0.866, 0,0.04, 0))
			game:GetService("TweenService"):Create(SearchBar,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{ImageTransparency = 0.5}):Play()
			game:GetService("TweenService"):Create(SettingsBar,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{ImageTransparency = 0}):Play()
			game:GetService("TweenService"):Create(SearchBar,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{ImageColor3=Color3.new(0.141176, 0.141176, 0.141176)}):Play()
		else
			SettingsBar.Visible = false
			SearchBar:TweenPosition(UDim2.new(0.927, 0,0.04, 0))
			game:GetService("TweenService"):Create(SettingsBar,TweenInfo.new(0.35,Enum.EasingStyle.Linear),{ImageTransparency = 1}):Play()
		end
	end
end)
--SearchScripts
local ObjectHolder = SearchLocal.Parent.Parent.Parent.Parent.Parent.ListElements.Frame
local Settings = SearchLocal.Parent.Parent.Parent.Parent.Parent.Menu.Settings.SettingsButtons.ScrollingFrame.Button1
local Visuals2 = Button2
local Panel2 = Button1
local Misc2 = Button3
local Settings2 = Button4
local SearchBarCheck = false
local Text2 = SearchLocal.Text
local Objects = {['TextLabel'] = true, ['ImageButton'] = true, ['ImageLabel'] = true, ['Frame'] = true}
local Type = 1
--[[
	Types:
	1 - Makes objects that do not fit search invisible
	2 - makes objects that fit search move higher on list, and those that don't go to the bottom
	
	
--]]



function Filter(Text2)
	for i,v in pairs(ObjectHolder:GetChildren()) do
		if Objects[v.ClassName] then
			if string.match(string.lower(v.Name), Text2) then
				if Type == 1 then
					v.Visible = true
					Settings.Visible = false
				end
				v.LayoutOrder = 1
			else
				if Type == 1 then
					v.Visible = false
					Settings.Visible = false
				end
				v.LayoutOrder = 0
			end
		end
	end
end


SearchLocal:GetPropertyChangedSignal('Text'):Connect(function()
	local CurrentText = SearchLocal.Text

	if CurrentText == "" then
		for i,v in pairs(ObjectHolder:GetChildren()) do
			if Objects[v.ClassName] then
				v.Visible = true
				Settings.Visible = true
			end
		end
	else
		Filter(string.lower(CurrentText))
	end
end)

local UIS = game:GetService('UserInputService')
local UI		= AdminHouse.Main.ColorPicker.ColorPicker
local sliders	= UI.Sliders.sliders
local preview	= Instances.Color
local SelectColorFrame = "Lines"
local FrameCheck2 = false
local preview2	= UI.preview.pre
local MainGui = UI.Parent.Parent.MainGui
local ColorChanger	= Instances.Color3
local LinesColor = Instances.Color4
local ColorSelect1 = UI.Frame.Frame2.MainButton
local ColorSelect2 = UI.Frame.Frame2.LineButton
local ColorSelect3 = UI.Frame.Frame2.IconButton
local ColorSelect4 = UI.Frame.Frame2.TextButton
local LeftBackground = UI.Parent.Parent.MainGui.SideBars.Background.Frame
local RightBackground = UI.Parent.Parent.MainGui.SideBars.Background.LeftBar
local inputs	= UI.inputs

-- top slider
local topArea	= sliders.topSlider
local topPicker	= topArea.Picker
local topGrad	= topArea.gradient.UIGradient

-- bottom slider
local botArea	= sliders.bottomSlider
local botPicker	= botArea.Picker
local botGrad	= botArea.gradient.UIGradient

-- inputs
local Rin		= inputs.RGB.R.TextBox.input
local Gin		= inputs.RGB.G.TextBox.input
local Bin		= inputs.RGB.B.TextBox.input
local HEXin 	= inputs.HEX.TextBox.input

-- misc
local selecting = false
local lastBotXPos = .5

-------------------------------------------------------------------
---- FUNCTIONS

local getColor = function(percentage, ColorKeyPoints)
	if (percentage < 0) or (percentage>1) then
		--error'getColor percentage out of bounds!'
		warn'getColor got out of bounds percentage (less than 0 or greater than 1'
	end

	local closestToLeft = ColorKeyPoints[1]
	local closestToRight = ColorKeyPoints[#ColorKeyPoints]
	local LocalPercentage = .5
	local color = closestToLeft.Value

	-- This loop can probably be improved by doing something like a Binary search instead
	-- This should work fine though
	for i=1,#ColorKeyPoints-1 do
		if (ColorKeyPoints[i].Time <= percentage) and (ColorKeyPoints[i+1].Time >= percentage) then
			closestToLeft = ColorKeyPoints[i]
			closestToRight = ColorKeyPoints[i+1]
			LocalPercentage = (percentage-closestToLeft.Time)/(closestToRight.Time-closestToLeft.Time)
			color = closestToLeft.Value:lerp(closestToRight.Value,LocalPercentage)
			return color
		end
	end
	warn('Color not found!')
	return color
end

local updateFromSlider = function(activeBar, xPos)
	if activeBar == 'top' then

		-- set the color of the bottom picker
		local TopColorKeyPoints = topGrad.Color.Keypoints
		local BottomGradColor = ColorSequence.new{
			ColorSequenceKeypoint.new(0,Color3.fromRGB(255,255,255)),
			ColorSequenceKeypoint.new(0.5,getColor(xPos,TopColorKeyPoints)),
			ColorSequenceKeypoint.new(1,Color3.fromRGB(0,0,0))
		}
		botGrad.Color = BottomGradColor

	else
		-- set the x position to calcualte the color
		lastBotXPos = xPos
	end

	-- get the current color from the bottom slider
	local currentColor = getColor(lastBotXPos, botGrad.Color.Keypoints)

	-- set the color of the preview circle
	preview.Value = currentColor
	preview2.BackgroundColor3 = currentColor
	ColorChanger.Value = currentColor
	if SelectColorFrame=="Lines" then
		MainGui.Lines.Line1.LocationOfLine.Line1.BackgroundColor3 = currentColor
		MainGui.Lines.Line1.LocationOfLine.Line4.BackgroundColor3 = currentColor
		MainGui.Lines.Line1.LocationOfLine.Line6.BackgroundColor3 = currentColor
		MainGui.Lines.Line2.Line2.BackgroundColor3 = currentColor
		MainGui.Lines.Line3.LocationOfLine.Line2.BackgroundColor3 = currentColor
		MainGui.Lines.Line3.LocationOfLine.Line3.BackgroundColor3 = currentColor
		MainGui.Lines.Line3.LocationOfLine.Line4.BackgroundColor3 = currentColor
		UI.Parent.Parent.MainGui.Menu.Settings.SettingsButtons.ScrollingFrame.ScrollBarImageColor3 = currentColor
		MainGui.Menu.Panel.PanelButtons.ScrollingFrame.Button3.Bar.BackgroundColor3 = currentColor
		MainGui.Menu.Panel.PanelButtons.ScrollingFrame.Button3.Bar.Frame.Button.Bar.BackgroundColor3 = currentColor
		MainGui.Menu.Panel.PanelButtons.ScrollingFrame.Button3.Bar.Frame.Button.BackgroundColor3 = currentColor
		MainGui.Menu.Panel.PanelButtons.ScrollingFrame.Button4.Bar.BackgroundColor3 = currentColor
		MainGui.Menu.Panel.PanelButtons.ScrollingFrame.Button4.Bar.Frame.Button.Bar.BackgroundColor3 = currentColor
		MainGui.Menu.Panel.PanelButtons.ScrollingFrame.Button4.Bar.Frame.Button.BackgroundColor3 = currentColor
		MainGui.Menu.Settings.SettingsButtons.ScrollingFrame.Button3.Bar.BackgroundColor3 = currentColor
		MainGui.Menu.Settings.SettingsButtons.ScrollingFrame.Button3.Bar.Frame.Button.Bar.BackgroundColor3 = currentColor
		MainGui.Menu.Settings.SettingsButtons.ScrollingFrame.Button3.Bar.Frame.Button.BackgroundColor3 = currentColor
		MainGui.Menu.Panel.PanelButtons.ScrollingFrame.Button6.Bar.BackgroundColor3 = currentColor
		MainGui.Menu.Panel.PanelButtons.ScrollingFrame.Button6.Bar.Frame.Button.Bar.BackgroundColor3 = currentColor
		MainGui.Menu.Panel.PanelButtons.ScrollingFrame.Button6.Bar.Frame.Button.BackgroundColor3 = currentColor
		--Lines
	else
		if SelectColorFrame=="MainGui" then
			MainGui.BackgroundColor3 = currentColor
			LeftBackground.BackgroundColor3 = currentColor
			RightBackground.BackgroundColor3 = currentColor
			Color2.Value = currentColor
		else
			if SelectColorFrame=="Icon" then
				MainGui.Logo.ImageColor3 = currentColor
				MainGui.SideBars.Buttons.Button1.Button.Icon.ImageColor3 = currentColor
				MainGui.SideBars.Buttons.Button2.Button.Icon.ImageColor3 = currentColor
				MainGui.SideBars.Buttons.Button3.Button.Icon.ImageColor3 = currentColor
				MainGui.SideBars.Buttons.Button4.Button.Icon.ImageColor3 = currentColor
				MainGui.SideBars.Buttons.Button5.Button.Icon.ImageColor3 = currentColor
				MainGui.Logo2.ImageColor3 = currentColor
				LeftBarButtonsShow.Value = currentColor
			else
				if SelectColorFrame=="Texts" then
					MainGui.SideBars.Buttons.Button1.Button.Text1.TextColor3 = currentColor
					MainGui.SideBars.Buttons.Button1.Button.Text2.TextColor3 = currentColor
					MainGui.SideBars.Buttons.Button2.Button.Text1.TextColor3 = currentColor
					MainGui.SideBars.Buttons.Button2.Button.Text2.TextColor3 = currentColor
					MainGui.SideBars.Buttons.Button3.Button.Text1.TextColor3 = currentColor
					MainGui.SideBars.Buttons.Button3.Button.Text2.TextColor3 = currentColor
					MainGui.SideBars.Buttons.Button4.Button.Text1.TextColor3 = currentColor
					MainGui.SideBars.Buttons.Button4.Button.Text2.TextColor3 = currentColor
					MainGui.SideBars.Buttons.Button5.Button.Text1.TextColor3 = currentColor
					MainGui.SideBars.Buttons.Button2.Button.Text2.TextColor3 = currentColor
					MainGui.Topbar.Texts.Text.TextColor3 = currentColor
					LeftBarButtonsTexts.Value = currentColor
				end
			end
		end
	end

	-- set the manual input values
	Rin.Text = string.split(tostring(currentColor.R * 255),".")[1]
	Gin.Text = string.split(tostring(currentColor.G * 255),".")[1]
	Bin.Text = string.split(tostring(currentColor.B * 255),".")[1]
	HEXin.Text = currentColor:ToHex()

end

local topSelection = function()
	selecting = true
	local ColorKeyPoints = topGrad.Color.Keypoints
	repeat wait()

		-- get the edge positions and calculate the full width of the area
		local minXPos = topArea.AbsolutePosition.X
		local maxXPos = minXPos+topArea.AbsoluteSize.X
		local xPixelSize = maxXPos-minXPos

		-- raw Mouse X pixel position
		local mouseX = UIS:GetMouseLocation().X

		-- constraints
		if mouseX<minXPos then
			mouseX = minXPos
		elseif mouseX>maxXPos then
			mouseX = maxXPos
		end

		-- get percentage mouse is on
		local xPos = (mouseX-minXPos)/xPixelSize

		-- move the visual Picker line
		topPicker.Position = UDim2.new(xPos,0,0,0)

		updateFromSlider('top', xPos)

	until not selecting
end

local botSelection = function()
	selecting = true
	local ColorKeyPoints = botGrad.Color.Keypoints
	repeat wait()

		-- get the edge positions and calculate the full width of the area
		local minXPos = botArea.AbsolutePosition.X
		local maxXPos = minXPos+botArea.AbsoluteSize.X
		local xPixelSize = maxXPos-minXPos

		-- raw Mouse X pixel position
		local mouseX = UIS:GetMouseLocation().X

		-- constraints
		if mouseX<minXPos then
			mouseX = minXPos
		elseif mouseX>maxXPos then
			mouseX = maxXPos
		end

		-- get percentage mouse is on
		local xPos = (mouseX-minXPos)/xPixelSize

		-- move the visual Picker line
		botPicker.Position = UDim2.new(xPos,0,0,0)

		updateFromSlider('bottom', xPos)

	until not selecting
end

local checkRGBvals = function(input)
	-- this will test if the input is a number
	-- and if the number between 0 and 255
	-- if either fails, return nil

	input = tonumber(input)
	if input ~= nil then
		if input < 0 or input > 255 then
			input = nil
		end
	end

	return input
end

local updateFromInput = function(inType)

	-- set vars
	local Rval, Gval, Bval, newColor
	local isValid = true

	if inType == 'HEX' then

		local HEXtest
		-- if the input is HEX, test it
		-- im using a pcall because i cant find a good way to test if a hex value is valid
		local success = pcall(function()
			HEXtest = Color3.fromHex(HEXin.Text)
		end)

		if success == false then
			isValid = false
		else
			Rval = math.floor(HEXtest.R*255)
			Gval = math.floor(HEXtest.G*255)
			Bval = math.floor(HEXtest.B*255)
		end

	elseif inType == 'RGB' then

		-- if the input is RGB, test it
		Rval = checkRGBvals(Rin.Text)
		Gval = checkRGBvals(Gin.Text)
		Bval = checkRGBvals(Bin.Text)

		if Rval == nil then
			isValid = false
		elseif Gval == nil then
			isValid = false
		elseif Bval == nil then
			isValid = false
		end

	end


	if isValid then
		-- if the input is valid, then set the new color to it
		newColor = Color3.fromRGB(Rval,Gval,Bval)

	else
		-- if the input is not valid, then set the new color to the preview color
		newColor = preview.Value
		newColor = preview2.BackgroundColor3
		newColor = ColorChanger.Value
		if SelectColorFrame=="Lines" then
			newColor = MainGui.Lines.Line1.LocationOfLine.Line1.BackgroundColor3
			newColor = MainGui.Lines.Line1.LocationOfLine.Line4.BackgroundColor3
			newColor = MainGui.Lines.Line1.LocationOfLine.Line6.BackgroundColor3
			newColor = MainGui.Lines.Line2.Line2.BackgroundColor3
			newColor = MainGui.Lines.Line3.LocationOfLine.Line2.BackgroundColor3
			newColor = MainGui.Lines.Line3.LocationOfLine.Line3.BackgroundColor3
			newColor = MainGui.Lines.Line3.LocationOfLine.Line4.BackgroundColor3
			newColor = UI.Parent.Parent.MainGui.Menu.Settings.SettingsButtons.ScrollingFrame.ScrollBarImageColor3
			newColor = MainGui.Menu.Panel.PanelButtons.ScrollingFrame.Button3.Bar.BackgroundColor3
			newColor = MainGui.Menu.Panel.PanelButtons.ScrollingFrame.Button3.Bar.Frame.Button.Bar.BackgroundColor3
			newColor = MainGui.Menu.Panel.PanelButtons.ScrollingFrame.Button3.Bar.Frame.Button.BackgroundColor3
			newColor = MainGui.Menu.Panel.PanelButtons.ScrollingFrame.Button4.Bar.BackgroundColor3
			newColor = MainGui.Menu.Panel.PanelButtons.ScrollingFrame.Button4.Bar.Frame.Button.Bar.BackgroundColor3
			newColor = MainGui.Menu.Panel.PanelButtons.ScrollingFrame.Button4.Bar.Frame.Button.BackgroundColor3
			newColor = MainGui.Menu.Settings.SettingsButtons.ScrollingFrame.Button3.Bar.BackgroundColor3
			newColor = MainGui.Menu.Settings.SettingsButtons.ScrollingFrame.Button3.Bar.Frame.Button.Bar.BackgroundColor3
			newColor = MainGui.Menu.Settings.SettingsButtons.ScrollingFrame.Button3.Bar.Frame.Button.BackgroundColor3
			newColor = MainGui.Menu.Panel.PanelButtons.ScrollingFrame.Button6.Bar.BackgroundColor3
			newColor = MainGui.Menu.Panel.PanelButtons.ScrollingFrame.Button6.Bar.Frame.Button.Bar.BackgroundColor3
			newColor = MainGui.Menu.Panel.PanelButtons.ScrollingFrame.Button6.Bar.Frame.Button.BackgroundColor3
		else
			if SelectColorFrame=="MainGui" then
				newColor = MainGui.BackgroundColor3
				newColor = LeftBackground.BackgroundColor3
				newColor = RightBackground.BackgroundColor3
				newColor = Color2.Value
			else
				if SelectColorFrame=="Icon" then
					newColor = MainGui.Logo.ImageColor3
					newColor = MainGui.SideBars.Buttons.Button1.Button.Icon.ImageColor3
					newColor = MainGui.SideBars.Buttons.Button2.Button.Icon.ImageColor3
					newColor = MainGui.SideBars.Buttons.Button3.Button.Icon.ImageColor3
					newColor = MainGui.SideBars.Buttons.Button4.Button.Icon.ImageColor3
					newColor = MainGui.SideBars.Buttons.Button5.Button.Icon.ImageColor3
					newColor = MainGui.Logo2.ImageColor3
					newColor = LeftBarButtonsShow.Value
				else
					if SelectColorFrame=="Texts" then
						newColor = MainGui.SideBars.Buttons.Button1.Button.Text1.TextColor3
						newColor = MainGui.SideBars.Buttons.Button1.Button.Text2.TextColor3
						newColor = MainGui.SideBars.Buttons.Button2.Button.Text1.TextColor3
						newColor = MainGui.SideBars.Buttons.Button2.Button.Text2.TextColor3
						newColor = MainGui.SideBars.Buttons.Button3.Button.Text1.TextColor3
						newColor = MainGui.SideBars.Buttons.Button3.Button.Text2.TextColor3
						newColor = MainGui.SideBars.Buttons.Button4.Button.Text1.TextColor3
						newColor = MainGui.SideBars.Buttons.Button4.Button.Text2.TextColor3
						newColor = MainGui.SideBars.Buttons.Button5.Button.Text1.TextColor3
						newColor = MainGui.SideBars.Buttons.Button2.Button.Text2.TextColor3
						newColor = MainGui.Topbar.Texts.Text.TextColor3
						newColor = LeftBarButtonsTexts.Value
					end
				end
			end
		end
	end

	-- apply new color to the text boxes and preview
	Rin.Text = math.floor(newColor.R*255)
	Gin.Text = math.floor(newColor.G*255)
	Bin.Text = math.floor(newColor.B*255)
	HEXin.Text = newColor:ToHex()
	preview.Value = newColor
	preview2.BackgroundColor3 = newColor
	ColorChanger.Value = newColor
	if SelectColorFrame=="Lines" then
		MainGui.Lines.Line1.LocationOfLine.Line1.BackgroundColor3 = newColor
		MainGui.Lines.Line1.LocationOfLine.Line4.BackgroundColor3 = newColor
		MainGui.Lines.Line1.LocationOfLine.Line6.BackgroundColor3 = newColor
		MainGui.Lines.Line2.Line2.BackgroundColor3 = newColor
		MainGui.Lines.Line3.LocationOfLine.Line2.BackgroundColor3 = newColor
		MainGui.Lines.Line3.LocationOfLine.Line3.BackgroundColor3 = newColor
		MainGui.Lines.Line3.LocationOfLine.Line4.BackgroundColor3 = newColor
		UI.Parent.Parent.MainGui.Menu.Settings.SettingsButtons.ScrollingFrame.ScrollBarImageColor3 = newColor
		MainGui.Menu.Panel.PanelButtons.ScrollingFrame.Button3.Bar.BackgroundColor3 = newColor
		MainGui.Menu.Panel.PanelButtons.ScrollingFrame.Button3.Bar.Frame.Button.Bar.BackgroundColor3 = newColor
		MainGui.Menu.Panel.PanelButtons.ScrollingFrame.Button3.Bar.Frame.Button.BackgroundColor3 = newColor
		MainGui.Menu.Panel.PanelButtons.ScrollingFrame.Button4.Bar.BackgroundColor3 = newColor
		MainGui.Menu.Panel.PanelButtons.ScrollingFrame.Button4.Bar.Frame.Button.Bar.BackgroundColor3 = newColor
		MainGui.Menu.Panel.PanelButtons.ScrollingFrame.Button4.Bar.Frame.Button.BackgroundColor3 = newColor
		MainGui.Menu.Settings.SettingsButtons.ScrollingFrame.Button3.Bar.BackgroundColor3 = newColor
		MainGui.Menu.Settings.SettingsButtons.ScrollingFrame.Button3.Bar.Frame.Button.Bar.BackgroundColor3 = newColor
		MainGui.Menu.Settings.SettingsButtons.ScrollingFrame.Button3.Bar.Frame.Button.BackgroundColor3 = newColor
		MainGui.Menu.Panel.PanelButtons.ScrollingFrame.Button6.Bar.BackgroundColor3 = newColor
		MainGui.Menu.Panel.PanelButtons.ScrollingFrame.Button6.Bar.Frame.Button.Bar.BackgroundColor3 = newColor
		MainGui.Menu.Panel.PanelButtons.ScrollingFrame.Button6.Bar.Frame.Button.BackgroundColor3 = newColor
	else
		if SelectColorFrame=="MainGui" then
			MainGui.BackgroundColor3 = newColor
			LeftBackground.BackgroundColor3 = newColor
			RightBackground.BackgroundColor3 = newColor
			Color2.Value = newColor
		else
			if SelectColorFrame=="Icon" then
				MainGui.Logo.ImageColor3 = newColor
				MainGui.SideBars.Buttons.Button1.Button.Icon.ImageColor3 = newColor
				MainGui.SideBars.Buttons.Button2.Button.Icon.ImageColor3 = newColor
				MainGui.SideBars.Buttons.Button3.Button.Icon.ImageColor3 = newColor
				MainGui.SideBars.Buttons.Button4.Button.Icon.ImageColor3 = newColor
				MainGui.SideBars.Buttons.Button5.Button.Icon.ImageColor3 = newColor
				MainGui.Logo2.ImageColor3 = newColor
				LeftBarButtonsShow.Value = newColor
			else
				if SelectColorFrame=="Texts" then
					MainGui.SideBars.Buttons.Button1.Button.Text1.TextColor3 = newColor
					MainGui.SideBars.Buttons.Button1.Button.Text2.TextColor3 = newColor
					MainGui.SideBars.Buttons.Button2.Button.Text1.TextColor3 = newColor
					MainGui.SideBars.Buttons.Button2.Button.Text2.TextColor3 = newColor
					MainGui.SideBars.Buttons.Button3.Button.Text1.TextColor3 = newColor
					MainGui.SideBars.Buttons.Button3.Button.Text2.TextColor3 = newColor
					MainGui.SideBars.Buttons.Button4.Button.Text1.TextColor3 = newColor
					MainGui.SideBars.Buttons.Button4.Button.Text2.TextColor3 = newColor
					MainGui.SideBars.Buttons.Button5.Button.Text1.TextColor3 = newColor
					MainGui.SideBars.Buttons.Button2.Button.Text2.TextColor3 = newColor
					MainGui.Topbar.Texts.Text.TextColor3 = newColor
					LeftBarButtonsTexts.Value = newColor
				end
			end
		end
	end
end
-------------------------------------------------------------------
---- EVENTS

topArea.MouseButton1Down:Connect(function() topSelection() end)
topArea.InputEnded:Connect(function() selecting = false end)

botArea.MouseButton1Down:Connect(function() botSelection() end)
botArea.InputEnded:Connect(function() selecting = false end)

Rin.FocusLost:Connect(function() updateFromInput('RGB') end)
Gin.FocusLost:Connect(function() updateFromInput('RGB') end)
Bin.FocusLost:Connect(function() updateFromInput('RGB') end)

HEXin.FocusLost:Connect(function() updateFromInput('HEX') end)
