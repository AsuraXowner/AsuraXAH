	else
		FrameCheck=true
		ColorPicker.Frame.Frame2:TweenPosition(UDim2.new(0, 0,-1.75, 0))
		ColorPicker.Frame.Frame2:TweenSize(UDim2.new(0, 230,0, 57))
	end
end)
